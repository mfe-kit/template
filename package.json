{
  "name": "@mfe-kit/template",
  "version": "0.0.28",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/mfe-kit/template.git"
  },
  "scripts": {
    "prepare": "node .husky/install.mjs",
    "dev": "npm-run-all -pl dev:*",
    "dev:fe": "vite --force -c vite.config.frontend.js",
    "dev:be": "NODE_NO_WARNINGS=1 tsx watch --experimental-strip-types --env-file=./src/.env src/backend/server.ts",
    "build": "rm -rf dist && npm run build:be && npm run build:fe",
    "build:fe": "vite build --mode production -c vite.config.frontend.js",
    "build:be": "node --env-file=src/.env.production build.js && vite build -c vite.config.ssr.js",
    "test": "vitest --config vite.config.frontend.js run --coverage",
    "lint": "eslint src",
    "check": "prettier --ignore-path .gitignore --check src/** --log-level warn",
    "check:fix": "prettier --ignore-path .gitignore --write {src,test}/; eslint --fix src",
    "version:patch": "npm version patch --no-git-tag-version",
    "version:minor": "npm version minor --no-git-tag-version",
    "version:major": "npm version major --no-git-tag-version"
  },
  "dependencies": {
    "@mfe-kit/core": "github:mfe-kit/core#v0.4.2",
    "@hono/node-server": "^1.14.3"
  },
  "devDependencies": {
    "@mfe-kit/dev-tools": "github:mfe-kit/dev-tools#v0.5.7",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "@vitest/coverage-v8": "^3.2.4",
    "esbuild": "^0.25.8",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.3",
    "husky": "^9.1.7",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass": "^1.92.1",
    "tsx": "^4.20.3",
    "typescript": "~5.9.0",
    "typescript-eslint": "^8.37.0",
    "vite": "^7.1.4",
    "vitest": "^3.2.4"
  }
}
